!!!
%html
  %head
    %title= @page_title || "Print Page"
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    /= stylesheet_link_tag    'https://fonts.googleapis.com/icon?family=Material+Icons'
    /= stylesheet_link_tag    'https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css'
    /= javascript_include_tag 'http://ajax.googleapis.com/ajax/libs/jquery/1.5/jquery.min.js'
    = csrf_meta_tags
  %body{:style => 'font-size: 20px; background-color:white;', onload: "printpage()"}
    .container
      .img-overlap.img-trsp-2.text-center{:style => 'disabled:true'}= image_tag("logo-large.png")
      = yield    # <--
