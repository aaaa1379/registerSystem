= content_for :left_bar do
  #left_bar
    = render(:partial => '/admin/features/overview')

%h2.caption 學生年份
.divider
.header_information
  .green-text=flash[:notice]
  .red-text=flash[:error]

%ol.header_information
  %li 新增學生請先選擇新增的年份

.card-panel
  %table.highlight
    %col{:width => "15%"}/
    %col{:width => "10%"}/
    %col{:width => "50%"}/
    %tbody
      %tr
        %td 年份
        %td 封存
        %td 已成為研究生人數
        %td
      - @years.each do |year|
        = render(:partial => "admin/years/year", locals: {params_year: year.year, params_archive: year.archive, params_count: @students.where(:year => year.year).count} )